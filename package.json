{
  "name": "my-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "mkdir -p build/fonts && mkdir build/react/ && tsc && cp -r src/fonts build/ && cp src/react/index.css build/react/",
    "build:changelog": "bunx @discordx/changelog --src src",
    "dev": "bun run src/main.ts",
    "start": "bun run build/main.js",
    "watch": "bun --hot run src/main.ts",
    "prisma:generate": "bunx prisma generate",
    "prisma:studio": "bunx prisma studio",
    "prisma:migrate": "bunx prisma migrate dev --name init"
  },
  "dependencies": {
    "@discordx/importer": "^1.2.3",
    "@discordx/pagination": "^3.4.1",
    "@elysiajs/static": "^0.7.1",
    "@prisma/client": "5.3.1",
    "discord.js": "^14.13.0",
    "discordx": "^11.7.6",
    "elysia": "^0.7.15",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "node-cron": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "^0.10.8",
    "sharp": "^0.32.6"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.199",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.5.9",
    "@types/node-cron": "^3.0.9",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "bun-types": "latest",
    "dedent": "^1.5.1",
    "prettier": "^3.0.3",
    "prisma": "5.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
