{
  "compilerOptions": {
    "types": [
      "bun-types"
    ],
    "lib": [
      "esnext"
    ],
    "outDir": "build",
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "experimentalDecorators": true,
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}